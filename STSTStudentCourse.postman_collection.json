									"});\r",
									"\r",
									"pm.test(\"Response should be in JSON format\", function () {\r",
									"    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');\r",
									"});\r",
									"\r",
									"var jsonData;\r",
									"try {\r",
									"    jsonData = pm.response.json();\r",
									"    console.log(jsonData);\r",
									"    console.log(jsonData.apiVersion);\r",
									"    console.log(jsonData.title);\r",
									"    console.log(jsonData.description);\r",
									"    console.log(jsonData.paymentCurrency);\r",
									"    console.log(jsonData.categories);\r",
									"    console.log(jsonData.scopes);\r",
									"    console.log(jsonData.links);\r",
									"    \r",
									"    pm.test(\"body is correct \", function () {\r",
									"        pm.expect(jsonData.apiVersion).to.eql(\"8.5.3.0\");\r",
									"    });\r",
									"    pm.test(\"Body should contain apiVersion \", function () {\r",
									"        pm.expect(jsonData).to.have.property(\"apiVersion\", \"8.5.3.0\");\r",
									"    });\r",
									"    pm.test(\"Body matches string\", function () {\r",
									"        pm.expect(pm.response.text()).to.include(\"apiVersion\");\r",
									"        pm.expect(pm.response.text()).to.include(\"8.5.3.0\"); // Fixed to separate the checks\r",
									"    });\r",
									"    pm.test(\"Body matches string\", function () {\r",
									"        pm.expect(pm.response.text()).to.include(\"EUR\");\r",
									"    });\r",
									"} catch (e) {\r",
									"    pm.test(\"Response is not valid JSON\", function () {\r",
									"        pm.expect.fail(\"JSONError: Unexpected token '<' at 1:1\");\r",
									"    });\r",
									"}\r",
									"pm.test(\"Body should contain description \", function () {\r",
									"    pm.expect(jsonData).to.have.property(\"description\", \"Self-Hosted API Management for Builders.\");\r",
